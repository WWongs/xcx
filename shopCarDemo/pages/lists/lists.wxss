/* pages/list/list.wxss */
.bannerList .banner{
    width: 100%;
    height: 350rpx;
}
.bannerList image{
    display: block;
    width: 100%;
    height: 100%;
}
.goodItem .headerImg{
    width: 100%;
    height: 500rpx;
}
.goodItem .item{
    padding: 0 15rpx;
    margin-bottom: 10rpx;
}
.goodItem .item .good{
    width: 350rpx;
    display: inline-block;
    margin-right: 10rpx;
    margin-bottom: 20rpx;
}
.goodItem .item .good image{
    display: block;
    width: 100%;
    height: 350rpx;
}
.goodItem .item .good .goodIfo{
    display: block;
    text-align: center;
    margin: 15rpx auto;
    font-size: 24rpx;
}
.goodItem .item .goodRight{
    margin-right: 0;
    margin-left: 11rpx;
}


